<div class="container">
  <div class="row">
  <form class="col s12 m12">
    <div class="row">
      <div class="input-field col s12 m5">
        <i class="material-icons prefix">account_circle</i>
        <input id="icon_prefix" type="text" class="validate">
        <label for="icon_prefix left">First Name</label>
      </div>
      <div class="input-field col s12 m5">
        <input id="icon_prefix" type="text" class="validate">
        <label for="icon_prefix">Last Name</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12 m5">
        <i class="material-icons prefix">phone</i>
        <input id="icon_telephone" type="tel" class="validate">
        <label for="icon_telephone">Telephone</label>
      </div>
      <div class="input-field col s12 m5">
        <i class="material-icons prefix">email</i>
        <input id="icon_telephone" type="email" class="validate">
        <label for="icon_telephone">your@email.com</label>
      </div>
    </div>
    <div class="row">
      <div class="col s12 m4 offset-m1">
        <select class="browser-default ">
          <option value="" disabled selected>Choose your option to consult</option>
          <option value="1">Asistenza Previdenziale</option>
          <option value="2">Asistenza Agli Invalidi Civili Sociale</option>
          <option value="3">Assistenza Danni Alla Salute Causati Dal Lavoro</option>
          <option value="2">Sportello Immigrazione</option>
        </select>
      </div>
      <!-- Date to pickup -->
      <div class="col s12 m4 offset-m1">
        <input type="date" class="datepicker">
      </div>
    </div>
      <div class="input-field col s12 m10">
        <i class="material-icons prefix">mode_edit</i>
        <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
        <label for="icon_prefix2">Your Message</label>
      </div>
      <div class="col s12 m5 file-field input-field">
        <div class="btn">
          <span>File</span>
          <input type="file" multiple>
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate" type="text" placeholder="Upload one file of your document">
        </div>
        <a class="waves-effect waves-light btn">Send</a>
      </div>

  </form>
</div>

</div>
